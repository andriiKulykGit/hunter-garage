extends layouts/layout.pug

block title
	- var title = "Hunters Garage на Сумській";

block content
	.single
		.container
			.single__body
				.single__breadcrumb
					include ./blocks/breadcrumb
				section
					.single__title
						h1.h1 Hunters Garage на Сумській
					.single__portfolio(id="portfolio-gallery")
